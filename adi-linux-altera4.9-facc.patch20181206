 	priv->dmaops->enable_rxirq(priv);
 	priv->dmaops->enable_txirq(priv);
+#endif
 
 	/* Setup RX descriptor chain */
 	for (i = 0; i < priv->rx_ring_size; i++)
@@ -1236,7 +1370,9 @@ static int tse_shutdown(struct net_device *dev)
