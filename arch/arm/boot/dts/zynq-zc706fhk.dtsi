					};
					sensor@48 {
						compatible = "ti,tmp75";// hwmon/lm75.c
						reg = <0x48>;
					};
				};*/

				/*i2c@4 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <4>;
					rtc@51 {
						compatible = "nxp,pcf8563";
						reg = <0x51>;
					};
					rtc@68 {
						compatible = "idt,idt1337";// driver unknown
						reg = <0x68>;
					};
				};*/
			};
		};
/*
