		//F=N'/8*M*S/L: case1/orx-stitching=(16/8)*2*1/2=2; case2/orx-2chn=(16/8)*4*1/2=4;
		adi,octets-per-frame = <2>;
		adi,frames-per-multiframe = <32>;
	};

